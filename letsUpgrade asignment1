//Q2. Program to convert minutes to seconds

let minutes = 30;
seconds = minutes * 60;
console.log("30 " + " Min " + " into sec is " + seconds);

//Q3. Program to search element in a array of strings

var array = ["a","n","r","s","m"];
console.log(array.indexOf("r"));


//Q4. Program to display only elements containing 'a' in the term of array

let arrays =["apps","apple","Fruits","Banana","Cat","Dog"];
for(let i=0;i<arrays.length;i++)
{
	if((arrays[i].search("a"))>-1)
	{
		console.log(arrays[i]);
	}
}

//Q5. Print an array in reverse order

Array = ["Animal","Fruits","Vegetables","plants"];
console.log(Array);
Array = ["Animal","Fruits","Vegetables","plants"].reverse();
console.log(Array);

